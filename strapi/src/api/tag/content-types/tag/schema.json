{
  "kind": "collectionType",
  "collectionName": "tags",
  "info": { "singularName": "tag", "pluralName": "tags", "displayName": "Tag" },
  "options": { "draftAndPublish": false },
  "pluginOptions": { "i18n": { "localized": true } },
  "attributes": {
    "name": { "type": "string", "required": true, "pluginOptions": { "i18n": { "localized": true } } },
    "slug": { "type": "uid", "targetField": "name", "pluginOptions": { "i18n": { "localized": true } } },
    "posts": { "type": "relation", "relation": "manyToMany", "target": "api::blog-post.blog-post", "mappedBy": "tags" }
  }
}
